<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="@dimen/paddinglr"
    android:layout_marginTop="@dimen/paddinglr"
    android:orientation="vertical" >

    <LinearLayout style="@style/home_btn_layout3" >

        <LinearLayout
            android:id="@+id/btn_10"
            style="@style/home_btn_style"
            android:background="@color/home_btn_c00" >

            <ImageView
                style="@style/home_btn_img"
                android:src="@drawable/homeimg_10" />

            <TextView
                style="@style/home_btn_text"
                android:text="@string/home_btn_10" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_11"
            style="@style/home_btn_style"
            android:background="@color/home_btn_09c" >

            <ImageView
                style="@style/home_btn_img"
                android:src="@drawable/homeimg_11" />

            <TextView
                style="@style/home_btn_text"
                android:text="@string/home_btn_11" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/btn_12"
            style="@style/home_btn_style"
            android:background="@color/home_btn_93c" >

            <ImageView
                style="@style/home_btn_img"
                android:src="@drawable/homeimg_12" />

            <TextView
                style="@style/home_btn_text"
                android:text="@string/home_btn_12" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout style="@style/home_btn_layout3" >

        <Button
            android:id="@+id/btn_13"
            style="@style/home_btn_style"
            android:background="@color/huan_s"
            android:text="@string/home_btn_13"
