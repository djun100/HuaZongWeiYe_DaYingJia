            android:id="@+id/btn_15"
            style="@style/home_btn_style"
            android:background="@color/huan_q"
            android:text="@string/home_btn_15"
            android:visibility="invisible" />
    </LinearLayout>

    <LinearLayout style="@style/home_btn_layout3" >

        <Button
            android:id="@+id/btn_16"
            style="@style/home_btn_style"
            android:background="@color/zi_q"
            android:text="@string/home_btn_16"
            android:visibility="invisible" />

        <Button
            android:id="@+id/btn_17"
            style="@style/home_btn_style"
            android:background="@color/lv_q"
            android:text="@string/home_btn_17"
            android:visibility="invisible" />

        <Button
            android:id="@+id/btn_18"
            style="@style/home_btn_style"
            android:background="@color/lan_q"
            android:text="@string/home_btn_18"
            android:visibility="invisible" />
